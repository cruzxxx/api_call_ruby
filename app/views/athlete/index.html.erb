<h1>Athlete Data</h1>

<table>
  <thead>
    <tr>
      <th style="border: 1px solid;">Athlete</th>
      <th style="border: 1px solid;">Test</th>
      <th style="border: 1px solid;">Value</th>
    </tr>
  </thead>
  <tbody>
    <% @athletes.each do |individual_athlete| %>
      <tr>
        <td style="border: 1px solid;">
          <%= individual_athlete['athlete'] %>
        </td>
        <td style="border: 1px solid;">
          <%= individual_athlete['test'] %>
        </td>
        <td style="border: 1px solid;">
          <%= individual_athlete['summary'] %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>